{
    "env": {
        "browser": true,
        "es2021": true
    },
    // "parser": "babel-eslint",
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "globals": {
        "document": true
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "overrides": [
        {
            "files": [
                "*.jsx",
                "*.js"
            ]
        }
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": [2, "tab"],
        "arrow-body-style": 0,
        "react/prop-types": 0,
        "no-tabs": 0,
        "react/no-array-index-key": 0
        // "no-unused-vars": 0
        // "react/function-component-definition": 0
        
    }
}
